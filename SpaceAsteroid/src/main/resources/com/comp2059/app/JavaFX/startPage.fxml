<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1200.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.comp2059.app.SpaceAsteroidController">
   <children>
      <ImageView fx:id="ImageShow" fitHeight="452.0" fitWidth="733.0" layoutX="64.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/artwork.png" />
         </image></ImageView>
      <Button fx:id="Play" layoutX="899.0" layoutY="143.0" mnemonicParsing="false" onMouseClicked="#onClickMoveToNameAndRule" prefHeight="81.0" prefWidth="124.0" text="PLAY">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="Exit" layoutX="899.0" layoutY="367.0" mnemonicParsing="false" onMouseClicked="#shotDown" prefHeight="81.0" prefWidth="124.0" text="EXIT">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button fx:id="skin" layoutX="254.0" layoutY="517.0" mnemonicParsing="false" onMouseClicked="#onClickMoveToSkinChange" prefHeight="69.0" prefWidth="114.0" text="Ship Skin" />
      <Button fx:id="background" layoutX="520.0" layoutY="517.0" mnemonicParsing="false" onMouseClicked="#onClickMoveToBackgroundChange" prefHeight="69.0" prefWidth="114.0" text="Background" />
      <Button fx:id="ranking" layoutX="724.0" layoutY="517.0" mnemonicParsing="false" onMouseClicked="#onClickMoveToRanking" prefHeight="69.0" prefWidth="114.0" text="Ranking" />
   </children>
</AnchorPane>
