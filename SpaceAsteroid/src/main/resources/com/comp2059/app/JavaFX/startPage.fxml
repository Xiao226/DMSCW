<AnchorPane prefHeight="720.0" prefWidth="1200.0" style="-fx-background-color: #708090;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.comp2059.app.SpaceAsteroidController">
