<AnchorPane prefHeight="720.0" prefWidth="1200.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.comp2059.app.controller.SpaceAsteroidController">
   <children>
      <ImageView fx:id="ImageShow" fitHeight="508.0" fitWidth="712.0" layoutX="196.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/artwork.png" />
         </image></ImageView>
      <Button fx:id="Play" layoutX="990.0" layoutY="488.0" mnemonicParsing="false" onMouseClicked="#onClickMoveToNameAndRule" prefHeight="197.0" prefWidth="152.0" text="PLAY">
         <font>
            <Font size="31.0" />
         </font></Button>
      <Button fx:id="Exit" layoutX="43.0" layoutY="488.0" mnemonicParsing="false" onMouseClicked="#shotDown" prefHeight="197.0" prefWidth="128.0" text="EXIT">
         <font>
            <Font size="31.0" />
         </font></Button>
      <Button fx:id="appearance" layoutX="625.0" layoutY="540.0" mnemonicParsing="false" onMouseClicked="#onClickMoveToAppearanceChange" prefHeight="105.0" prefWidth="326.0" text="Appearance">
         <font>
            <Font size="32.0" />
         </font></Button>
      <Button fx:id="ranking" layoutX="243.0" layoutY="542.0" mnemonicParsing="false" onMouseClicked="#onClickMoveToRanking" prefHeight="105.0" prefWidth="352.0" text="Ranking">
         <font>
            <Font size="32.0" />
         </font></Button>
      <ComboBox fx:id="module114514" layoutX="956.0" layoutY="297.0" onAction="#onMouseClickModel" prefHeight="62.0" prefWidth="219.0" promptText="Game Module Selection">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="Normal" />
               <String fx:value="Endless" />
            </FXCollections>
         </items>
      </ComboBox>
      <Button layoutX="1008.0" layoutY="123.0" mnemonicParsing="false" prefHeight="73.0" prefWidth="114.0" text="Info" />
   </children>
</AnchorPane>
